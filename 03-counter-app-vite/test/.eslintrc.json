{
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:jest/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": ["react", "react-hooks", "jest"],
    "rules": {
      // Aquí puedes agregar reglas personalizadas si lo necesitas
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
  